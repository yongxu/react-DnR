{
  "name": "react-dnr",
  "version": "0.2.2",
  "description": "react-DnR, dragable and resizable window",
  "files": [
    "*.md",
    "docs",
    "es6",
    "lib",
    "modules"
  ],
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "scripts": {
    "build": "npm run build-babel && npm run build-es6",
    "build-babel": "rimraf lib && babel ./modules -d lib",
    "build-es6": "rimraf es6 && babel ./modules -d es6 --blacklist=es6.modules",
    "build-umd": "webpack --config webpack.build.config.js modules/index.jsx umd/DnR.js",
    "start": "webpack-dev-server --progress --colors",
    "gh-pages": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yongxu/react-DnR.git"
  },
  "author": "Yongxu Ren",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yongxu/react-DnR/issues"
  },
  "homepage": "https://github.com/yongxu/react-DnR",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  }
}
